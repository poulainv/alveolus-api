        <input type="text" id="log"/>

        <input type="password" id="pwd"/>

        <script>
        var url="http://localhost:3000"
        var auth_token = ""; 
        function login() {
          var url="http://localhost:3000"
          console.log("goo in");
          var email = $('#log').val();
          var password = $('#pwd').val();
          console.log(password);
          var url = '';
          var data = {remote: true, commit: "Sign in", utf8: "âœ“", remember_me: 1, password: password, email: email};
          $.post('http://localhost:3000/users/sign_in.json', data, function(resp) {
            console.log(resp);
            if(resp.success) {
              auth_token = resp.auth_token;
              console.log("ok");
            } else {
              console.log("ko");
      // let the user know they failed authentication
    }
  });
          return false;
        }

        function test() {
          var url="http://localhost:3000"
          var data = {auth_token:"auth_token"};
          console.log("goo tes token : "+auth_token);
          $.post(url+'/webapps', data, function(resp) {
            console.log(resp);
            if(resp.success) {
              console.log("ok");
            } else {
              console.log("ko");
            }
          }, function(d){
              console.log("eror");
            }
          );
          return false;
        }

        function logout() {
          var url="http://localhost:3000"
          console.log("goo out");
          var url = '';
          $.ajax({            url: url+"/users/sign_out.json",
            type : 'DELETE'
          }).done(function(data) {
            console.log(data);
          });
          return false;
        }
        </script>
