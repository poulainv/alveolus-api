<h2><%=@title%></h2>



<%= form_for(@webapp) do |f| %>
<%= render 'shared/error_messages' %>
<legend>Soumettez une App</legend>
<div class="offset2">
  <div class="field">
    <%= f.label :titre %>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :legende %>
    <%= f.text_field :caption %>
  </div>
  <div class="field">
    <%= f.label :description %>
    <%= f.text_area :description, :size=> "40x12" %>
  </div>
  <div class="field">
    <%= f.label :url, "URL" %>
    <%= f.text_field :url %>
  </div>
  
  <div class="actions">
    <%= f.submit "Ajout" %>
  </div>


<% end %>

</div>