# and open the template in the editor.
%>



<div class="container-fluid hover_green websiteTitle" websiteId="<%= websites.id %>" data-toggle="modal">
  <div class="span3">
    <h4> <%= websites_counter + 1%></h4>
  </div>

  <div class="span9">
    <div class="row-fluid">
      <li><a>
          <%= websites.title %>
        </a></li>
    </div>
    <div class="row-fluid">
    <%=  render :partial => "shared/star_rating", :locals => { :n => websites.average_rate }%>
  </div>
</div>
</div>

<div class="line-separator-dashed"></div>
