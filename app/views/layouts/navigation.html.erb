<!-- This the template html used for result of search website -->
<!-- Template with just topbar/menubar/mainview -->
<!doctype html>
<html>
  <head>
    <title>EnjoyTheWeb - Surfer autrement !</title>
    <%= render('layouts/skeleton/headers')%>
    <%= csrf_meta_tags %>
  </head>

  <body>

    <%= render('layouts/skeleton/topbar') %>

    <div class="row-fluid">
      <div class="span7 offset2">
        <%=  render :partial => "layouts/skeleton/menubar" %>
      </div>   
    </div>
    <div class="container-fluid" style="margin-top: 2%">
      <div class="row-fluid" > 
        <div class="span10 offset1">
          <%= render 'shared/flash_messages' %>
          <div class="hero-unit">
            <%= yield %>
          </div>
        </div>
      </div>
      <%= render 'layouts/detail_website_popup/popup_website' %>
    </div><!--/.fluid-container-->

    <%= render 'layouts/skeleton/footer' %>
    <%= debug(params) if Rails.env.development? %>
  </body>
</html>